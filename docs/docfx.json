{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/overrides"
    ],
    "globalMetadata": {
      "_appTitle": "TechTolk",
      "_appName": "TechTolk",
      "_appLogoPath": "images/techtolk-logo.svg",
      "_appFaviconPath": "images/techtolk-logo-icon.svg",
      "_enableSearch": true,
      "pdf": false
    }
  }
}